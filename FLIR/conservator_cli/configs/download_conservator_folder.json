{
  "name": "Download conservator folder example",
  "description": "Download a conservator folder",
  "plugin_registry": { "$ref": "$NTKROOT/conservator-cli/FLIR/conservator_cli/run_tasks_support/valid_tasks.json"},
  "tasks": [
    {
      "name": "get_creds",
      "description": "Load credential settings (email, API token) from JSON file",
      "task_type": "load_credentials",
      "task_config": {
        "cred_filename": "/tmp/creds.json"
      }
    },
    {
      "name": "download_folder",
      "description": "Download folder from Conservator",
      "task_type": "conservator_download_folder",
      "depends": ["get_creds"],
      "task_config": {
        "local_folder": "/mnt/data/local_datasets/download_folder_test",
        "conservator_folder": "/AndresTest",
        "include_video_metadata": true,
        "include_associated_files": true,
        "include_media": true
      }
    }
  ]
}
